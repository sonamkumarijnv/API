{
  "name": "express-api-template",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "nodemon --experimental-modules --es-module-specifier-resolution=node ./src/bin/www.js",
    "prestart": "babel ./src --out-dir build",
    "startdev": "nodemon --experimental-modules --es-module-specifier-resolution=node ./src/bin/www.js",
    "lint": "./node_modules/.bin/eslint ./src",
    "pretty": "prettier --write '**/*.{js,json}' '!node_modules/**'",
    "postpretty": "npm lint --fix",
    "test": "nyc --reporter=html --reporter=text --reporter=lcov mocha -r @babel/register",
    "runQuery": "babel-node ./src/utils/runQuery"
  },
  "dependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/node": "^7.16.8",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.0",
    "@babel/runtime": "^7.17.2",
    "chai": "^4.3.6",
    "cookie-parser": "~1.4.4",
    "coveralls": "^3.1.1",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "mocha": "^9.2.1",
    "morgan": "~1.9.1",
    "nyc": "^15.1.0",
    "pg": "^8.7.3",
    "prettier": "^2.5.1",
    "sinon-chai": "^3.7.0",
    "supertest": "^6.2.2"
  }
}
